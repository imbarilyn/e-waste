    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-restart": "^0.4.2",
